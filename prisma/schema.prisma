// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema
datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id         String @id @default(cuid())
  amazonAsin String
  clicks     Int
  websiteId  String

  @@index([websiteId])
}

model Website {
  id          String @id @default(cuid())
  websiteName String
  visits      Int
}
